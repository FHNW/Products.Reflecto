<?xml version="1.0"?>
<!-- Register the local Reflecto configuration utility. This is modified via
     a control panel page in browser/config.py.
  -->
<componentregistry>
 <utilities>  
  <utility
     interface="Products.Reflecto.interfaces.IReflectoConfiguration"
     factory="Products.Reflecto.utilities.ReflectoConfiguration"
     name="reflecto_config"
     />
 </utilities>
</componentregistry>
